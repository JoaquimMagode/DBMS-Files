CREATE TABLE Salesman (
    Sl_no VARCHAR(10) PRIMARY KEY,
    Name VARCHAR(50),
    Add1 VARCHAR(100),
    Add2 VARCHAR(100),
    City VARCHAR(50),
    Pin VARCHAR(10),
    State VARCHAR(50),
    Amt DECIMAL(10, 2),
    Tgt INT,
    Sales INT,
    Rem VARCHAR(10)
);

INSERT INTO Salesman (Sl_no, Name, Add1, Add2, City, Pin, State, Amt, Tgt, Sales, Rem) VALUES
