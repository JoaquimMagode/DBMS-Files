CREATE TABLE Employee (
    Emp_name VARCHAR(50),
    Street VARCHAR(50),
    City VARCHAR(50),
    PRIMARY KEY (Emp_name)
);
INSERT INTO Employee (Emp_name, Street, City) VALUES ('Adam', 'Spring', 'Pittsfield');
